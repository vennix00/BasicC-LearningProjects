

#include <stdio.h>

int main()
{
    int not;
    printf("Alınan notu giriniz: ");
    scanf("%d",&not);
    if(not<0 || not>100)
    {
        while(not>100 || not<0)
        {
        printf("0 ve 100 aralığında bir sayı giriniz.");
        scanf("%d",&not);
        }
    }
    if(not>0 && not<=40)
    {
        printf("D");
    }
    else if(not<=60)
    {
        printf("C");
    }
    else if(not<=80)
    {
        printf("B");
    }
    else
    {
        printf("A");
        
        return 0;
    }
}

